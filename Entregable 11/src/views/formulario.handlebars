<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
<script src="/socket.io/socket.io.js"></script>
    <h1>Productos Manager Online</h1>

    <h4>5 Productos Generados con faker.     En /normalizado Esta la data Normalizada en consola</h4>
    <form id="productosForm"></form>
    <div class="container">
      <table class="table table-hover table-bordered">
        <thead>
          <tr>
            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">Thumbnail</th>
          </tr>
        </thead>  
         <tbody>
           {{#each productsFaker}}
          <tr>
            <td>{{this.title}} </td>
            <td>{{this.prices}} </td>
            <td> <img src="{{this.thumbnail}}" alt="El enlace no esta disponible" width="60"></td>
          </tr>
          {{/each}}
        </tbody>


      </table>  
    </div>



    <h4>Chat</h4>
    <div class="container">
      </br>
      <span id="log"></span>
      <input id="chatBox">
    </div>

<script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
